{
  "name": "swim-gen-bff",
  "version": "1.0.0",
  "description": "A web application for generating, recommeding and sharing training plans for swimmers, triathletes and trainers.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "dev": "dotenv -e .env.development -- nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5pirit5eal/swim-gen.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/5pirit5eal/swim-gen/issues"
  },
  "homepage": "https://github.com/5pirit5eal/swim-gen#readme",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.5.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/eslint-plugin": "^1.5.2",
    "@vitest/ui": "^4.0.16",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-define-config": "^2.1.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  }
}
